    "@modern-js/tsconfig": "2.48.1",
    "husky": "8.0.0",
    "lint-staged": "~13.1.0",
    "prettier": "2.6.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "turbo": "^1.13.2",
    "whistle": "^2.9.67"
  },
  "volta": {
    "node": "18.20.2"
