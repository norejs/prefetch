{
  "private": true,
  "name": "prefetch",
  "scripts": {
    "new": "modern new",
    "reset": "modern clear",
    "change": "modern change",
    "bump": "modern bump",
    "pre": "modern pre",
    "prepare": "husky install",
    "change-status": "modern change-status",
    "gen-release-note": "modern gen-release-note",
    "release": "modern release",
    "deploy": "modern deploy",
    "lint": "modern lint",
