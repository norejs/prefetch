    "main": "index.js",
    "packageManager": "pnpm@7.33.7",
    "npmRegistry": "https://registry.npmjs.org",
    "scripts": {
        "test": "",
        "start": "turbo start",
        "build": "turbo build",
        "publish": "turbo publish",
        "changeset": "changeset",
        "changeset:version": "changeset version",
        "changeset:publish": "changeset publish",
        "changeset:status": "changeset status",
        "version-packages": "changeset version && pnpm install --lockfile-only",
        "release": "pnpm build && changeset publish",
        "demo:api": "cd demos/api-server && npm start",
        "demo:vite": "cd demos/vite-prefetch-demo && npm run dev",
        "demo:nextjs": "cd demos/nextjs-prefetch-demo && npm run dev",
        "demo:start:vite": "concurrently \"npm run demo:api\" \"npm run demo:vite\"",
        "demo:start:nextjs": "concurrently \"npm run demo:api\" \"npm run demo:nextjs\"",
        "demo:start:all": "concurrently \"npm run demo:api\" \"npm run demo:vite\" \"npm run demo:nextjs\"",
        "demo:install": "cd demos/api-server && npm install && cd ../vite-prefetch-demo && pnpm install && cd ../nextjs-prefetch-demo && pnpm install",
        "demo:copy-sw": "cd demos/nextjs-prefetch-demo && npm run copy-sw",
        "demo:build": "cd demos/vite-prefetch-demo && npm run build && cd ../nextjs-prefetch-demo && npm run build"
    },
